generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}

model user {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model file {
  id        Int      @id @default(autoincrement())
  filename  String
  mimetype  String
  size      Int
  path      String
  createdAt DateTime @default(now())
  userId    Int
  folderId  Int?
}
