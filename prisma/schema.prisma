generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}

model user {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}
